<%- include("header") -%>
<body>
    <div class="box" id="heading">
        <h1><%= listType %></h1>
    </div>
    
    <div class="box">

        <%newItem.forEach(function(item){%>
            <div class="item">
                <input type="checkbox">
                <p><%= item.name %> </p>
            </div>
        <%})%>
       
        <form action="/" method="post" class="item">
            <input type="text" name="listItem" autocomplete="off" placeholder="New Item">
            <button type="submit" name="type" value=<%= listType %>>
                +
            </button>
        </form>
       
    </div>
   
</body>
</html>